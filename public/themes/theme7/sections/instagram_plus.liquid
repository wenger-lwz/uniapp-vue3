{% javascript %}
  $(function () {
    var $section = $('[data-section-id=' + {{section.id}} + ']');
    var $container = $section.find('.ins_list');
    $(document).on('click', '[data-section-id=' + {{section.id}} + '] .ins_pagination', function () {
      var distance = $section.find('.ins_list > div > a:first').outerWidth() + 20;
      $container.stop().animate({
          scrollLeft: $container.scrollLeft() + ({'prev': -1, 'next': 1}[$(this).data('direction')] * distance)
        }, { duration: "150", easing: "linear" }
      );
    })
    $container.scroll($.throttle(function () {
      var direction = ($container[0].scrollWidth - ($container.width() + $container.scrollLeft()) <= 5 && 'next')
        || ($container.scrollLeft() <= 5 && 'prev') || 'none';
      $section.find('.ins_pagination[data-direction]')
        .removeClass('swiper-button-disabled').end()
        .find('.ins_pagination[data-direction='+direction+']')
        .addClass('swiper-button-disabled');
    }, 100))
  })
{% endjavascript %}
<style>
[data-section-type="instagram_plus"].xfcart-select:before {
    animation: none !important;
}
.instagram-feed-wrap {
    width: 100%;
    overflow: hidden;
    position: relative;
}
.HScroll-wrapper{
  position:relative;
  transition-property: left;
  transition-duration: 0.6s;
  transition-timing-function: cubic-bezier(0, 0, 0.15, 0.97);
}
.ins_pagination{
  width:43px;
  height:43px;
  background:rgba(50,50,50,1);
  box-shadow:0px 2px 10px 0px rgba(0,0,0,0.11);
  opacity:0.7;
  position:absolute;
  top:50%;
  margin-top:-21px;
  text-align:center;
  line-height:43px;
  z-index:10;
  color: #fff;
  border-radius: 50%;
  cursor: pointer;
}
.ins_pagination.sep-font-angle-left{
  left:36px;
}
.ins_pagination.sep-font-angle-right{
  right:36px;
}
.ins_container:hover .ins_pagination{
  opacity:0.7;
}
.ins_container .ins_pagination.swiper-button-disabled {
    cursor: not-allowed;
    background: rgba(204, 204, 204, 0.6);
}
.collection_slider__outer.ins_container {
    width: 100%;
    max-width: none;
    position: relative;
    overflow: hidden;
}
.instagram-section {
    padding: 80px 0;
    padding-bottom: 10px;
}

.instagram-section h2 {
    text-align: center;
    margin-bottom: 80px;
}
.ins_list{
  display:flex;
  -ms-overflow-style: none;
  overflow: -moz-scrollbars-none;
  overflow-x: auto;
}
.ins_list::-webkit-scrollbar { width: 0 !important }
.ins_item {
    width: 305px;
    background: #fff;
    margin: 0 10px;
    flex-shrink: 0;
    display: inline-block;
}

.ins_img {
    width: 100%;
    height: 325px;
    background-color: #F1F1F1;
    background-position: center;
    position: relative;
}

.ins_img i {
    position: absolute;
    width: 20px;
    height: 20px;
    right: 10px;
    top: 10px;
    font-size: 20px;
    line-height: 20px;
    color: #000;
    z-index: 5;
}

.in_user {
    padding: 10px 10px 10px 75px;
    position: relative;
    height:68px;
}

.in_user>div:first-child {
    width: 48px;
    height: 48px;
    background: #BFBFBF;
    border-radius: 50%;
    margin-right: 10px;
    position: absolute;
    left: 10px;
    top: 10px;
    display: flex;
    justify-content: center;
    align-items: center;
    overflow: hidden;
}
.in_user>div:first-child i {
    position: absolute;
    font-size: 30px;
    left: 0;
    bottom: 0;
    line-height: 48px;
    text-align: center;
    width: 48px;
    height: 48px;
    overflow: hidden;
    color: #fff;
}
.in_user>div:first-child img {
    max-width: 100%;
    position:relative;
    z-index:10;
}
.ins_name {
    font-size: 16px;
    font-family:PingFangSC-Medium,PingFang SC;
    font-weight:500;
    color:rgba(85,87,90,1);
    line-height:22px;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    margin: 0 0  5px;
    height:22px;
}

.ins_id {
    font-size: 14px;
    font-family:Futura-Medium,Futura;
    font-weight:500;
    color:rgba(143,143,145,1);
    line-height:19px;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    flex: 1 1 auto;
    margin: 0;
}

.in_user>div:first-child i {
    width: 100%;
    height: 100%;
}
</style>
