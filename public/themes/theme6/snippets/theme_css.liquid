  font-size: 12px;
  color: #fff;
  text-align: center;
  padding: 2px 0;
  left: 0;
  bottom: 0;
}

.product-snippet__img-wrapper {
  display: block;
  overflow: hidden;
  position: relative;
}

.product-snippet__img-wrapper .product-snippet__mask {
  display: flex;
  position: absolute;
  z-index: 2;
  opacity: 0;
  top: 0;
  width: 100%;
  height: 100%;
