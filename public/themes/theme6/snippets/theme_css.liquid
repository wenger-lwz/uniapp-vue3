/* lazyload css start */
.common__img-wrapper_lazy {
  position: relative;
  display: block;
  width: 100%;
}

.common__img-wrapper_lazy img {
  max-width: 100%;
  height: 100%;
  max-height: 100%;
  position: absolute;
  margin: auto;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
}

